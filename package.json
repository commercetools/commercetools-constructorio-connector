{
  "name": "[[service_name]]",
  "version": "0.1.0",
  "description": "[[service_description]]",
  "watch": {
    "dev": "src"
  },
  "scripts": {
    "start": "dotenv -e config/${NODE_ENV:-development}.env -e ./.env node src/index.js",
    "dev": "dotenv -e config/${NODE_ENV:-development}.env -e ./.env nodemon src/index.js",
    "docs": "serve -p ${DOCSPORT:-5000} docs",
    "lint": "yarn run lint:js && yarn run lint:helm",
    "lint:js": "prettier-eslint $INIT_CWD/src/**/*.js",
    "lint:js:fix": "prettier-eslint --write $INIT_CWD/src/**/*.js",
    "lint:helm": "helm lint ${npm_package_name}",
    "build": "yarn run clean && yarn run webpack",
    "build:prod": "cross-env NODE_ENV=production yarn run build",
    "test": "cross-env NODE_ENV=development jest --passWithNoTests",
    "test:watch": "cross-env NODE_ENV=development jest --watch",
    "ci-lint": "yarn run lint",
    "ci-test": "yarn run test",
    "ci-package": "docker build --tag [[docker_image_url]]:[[docker_image_tag]] .",
    "ci-package-clean": "docker build --no-cache --tag [[docker_image_url]]:[[docker_image_tag]] .",
    "ci-push": "kubectl create namespace ${npm_package_name} && docker push [[docker_image_url]]:[[docker_image_tag]]",
    "ci-deploy": "helm upgrade ${npm_package_name} helm-chart --install --wait --force --namespace ${npm_package_name} --set image.tag=[[docker_image_tag]]",
    "ci-undeploy": "helm uninstall ${npm_package_name} --namespace ${npm_package_name}"
  },
  "engines": {
    "node": ">= 12"
  },
  "keywords": [
    "commercetools",
    "node",
    "microservices"
  ],
  "author": "commercetools",
  "license": "MIT",
  "repository": "",
  "devDependencies": {
    "@babel/core": "7.8.6",
    "@babel/plugin-proposal-optional-chaining": "7.8.3",
    "@babel/preset-env": "7.8.6",
    "babel-cli": "6.26.0",
    "babel-eslint": "10.1.0",
    "babel-jest": "25.1.0",
    "babel-loader": "8.0.6",
    "eslint": "^4.15.0",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-jest": "^21.6.2",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^7.5.1",
    "jest": "^22.1.1",
    "lint-staged": "^6.0.1",
    "nock": "^9.1.6",
    "npm-watch": "^0.3.0",
    "prettier": "^1.10.2",
    "prettier-eslint-cli": "^4.7.1",
    "validate-commit": "^3.4.0",
    "webpack": "^4.42.0",
    "webpack-cli": "^3.3.11"
  },
  "dependencies": {
    "@commercetools/api-request-builder": "^3.5.0",
    "@commercetools/sdk-client": "^1.5.3",
    "@commercetools/sdk-middleware-auth": "^3.4.4",
    "@commercetools/sdk-middleware-http": "^2.2.3",
    "@commercetools/sdk-middleware-queue": "^1.1.3",
    "@commercetools/sync-actions": "^2.2.1",
    "@firebase/app": "^0.5.5",
    "avatax": "^19.12.1",
    "body-parser": "^1.19.0",
    "cors": "^2.8.5",
    "cross-env": "^5.1.3",
    "ctvault": "^1.0.9",
    "dotenv": "^8.2.0",
    "dotenv-cli": "^3.1.0",
    "express": "^4.17.1",
    "fast-crc32c": "^2.0.0",
    "fastest-validator": "^0.6.5",
    "lodash": "^4.17.15",
    "lodash.difference": "^4.5.0",
    "lodash.flowright": "^3.5.0",
    "lodash.intersection": "^4.4.0",
    "lodash.isstring": "^4.0.1",
    "lodash.union": "^4.6.0",
    "lodash.uniq": "^4.5.0",
    "micro": "^9.1.0",
    "micro-cors": "^0.1.1",
    "microrouter": "^3.0.0",
    "nconf": "^0.10.0",
    "randomstring": "^1.1.5",
    "redoc": "^1.20.0",
    "serve": "^6.4.9",
    "slugify": "^1.3.3",
    "uuid": "^3.3.2",
    "winston": "^3.0.0-rc5"
  },
  "jest": {
    "rootDir": "src",
    "collectCoverage": true,
    "coverageReporters": [
      "lcov",
      "text-summary"
    ],
    "coverageDirectory": "../coverage",
    "collectCoverageFrom": [
      "services/**/*.js"
    ],
    "testEnvironment": "node"
  },
  "lint-staged": {
    "src/**/*.{js,json}": [
      "prettier-eslint --write",
      "git add"
    ]
  }
}
